section#z-index
  h2 _z-index.less
  section
    h3 all media queries
    div(class="!z:0 ")   .!z:0    { z-index: initial !important;}
    div(class="!z:i ")   .!z:i    { z-index: inherit !important;}
    div(class="!z:a ")   .!z:a    { z-index: auto    !important;}
    div(class="!z:z ")   .!z:z    { z-index:  0      !important;}
    div(class="!z:1 ")   .!z:1    { z-index:  1      !important;}
    div(class="!z:2 ")   .!z:2    { z-index:  2      !important;}
    div(class="!z:3 ")   .!z:3    { z-index:  3      !important;}
    div(class="!z:4 ")   .!z:4    { z-index:  4      !important;}
    div(class="!z:5 ")   .!z:5    { z-index:  5      !important;}
    div(class="!z:6 ")   .!z:6    { z-index:  6      !important;}
    div(class="!z:7 ")   .!z:7    { z-index:  7      !important;}
    div(class="!z:-1")   .!z:-1   { z-index: -1      !important;}
    div(class="!z:-2")   .!z:-2   { z-index: -2      !important;}
    div(class="!z:-3")   .!z:-3   { z-index: -3      !important;}
    div(class="!z:-4")   .!z:-4   { z-index: -4      !important;}
    div(class="!z:-5")   .!z:-5   { z-index: -5      !important;}
    div(class="!z:-6")   .!z:-6   { z-index: -6      !important;}
    div(class="!z:-7")   .!z:-7   { z-index: -7      !important;}
  section
    h3 @media screen
    div(class="!z:0@s")  .!z:0@s  { z-index: initial !important;}
    div(class="!z:i@s")  .!z:i@s  { z-index: inherit !important;}
    div(class="!z:a@s")  .!z:a@s  { z-index: auto    !important;}
    div(class="!z:z@s")  .!z:z@s  { z-index:  0      !important;}
    div(class="!z:1@s")  .!z:1@s  { z-index:  1      !important;}
    div(class="!z:2@s")  .!z:2@s  { z-index:  2      !important;}
    div(class="!z:3@s")  .!z:3@s  { z-index:  3      !important;}
    div(class="!z:4@s")  .!z:4@s  { z-index:  4      !important;}
    div(class="!z:5@s")  .!z:5@s  { z-index:  5      !important;}
    div(class="!z:6@s")  .!z:6@s  { z-index:  6      !important;}
    div(class="!z:7@s")  .!z:7@s  { z-index:  7      !important;}
    div(class="!z:-1@s") .!z:-1@s { z-index: -1      !important;}
    div(class="!z:-2@s") .!z:-2@s { z-index: -2      !important;}
    div(class="!z:-3@s") .!z:-3@s { z-index: -3      !important;}
    div(class="!z:-4@s") .!z:-4@s { z-index: -4      !important;}
    div(class="!z:-5@s") .!z:-5@s { z-index: -5      !important;}
    div(class="!z:-6@s") .!z:-6@s { z-index: -6      !important;}
    div(class="!z:-7@s") .!z:-7@s { z-index: -7      !important;}

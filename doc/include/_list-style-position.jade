section#list-style-position
  h2 _list-style-position.less
  section
    h3 all media queries
    div(class="!d:li !lsp:0 ")   .!lsp:0    { list-style-position: initial !important; }
    div(class="!d:li !lsp:i ")   .!lsp:i    { list-style-position: inherit !important; }
    div(class="!d:li !lsp:os")   .!lsp:os   { list-style-position: outside !important; }
    div(class="!d:li !lsp:is")   .!lsp:is   { list-style-position: inside  !important; }
  section
    h3 @media screen
    div(class="!d:li !lsp:0@s ") .!lsp:0@s  { list-style-position: initial !important; }
    div(class="!d:li !lsp:i@s ") .!lsp:i@s  { list-style-position: inherit !important; }
    div(class="!d:li !lsp:os@s") .!lsp:os@s { list-style-position: outside !important; }
    div(class="!d:li !lsp:is@s") .!lsp:is@s { list-style-position: inside  !important; }
  section
    h3 @media print
    div(class="!d:li !lsp:0@p ") .!lsp:0@p  { list-style-position: initial !important; }
    div(class="!d:li !lsp:i@p ") .!lsp:i@p  { list-style-position: inherit !important; }
    div(class="!d:li !lsp:os@p") .!lsp:os@p { list-style-position: outside !important; }
    div(class="!d:li !lsp:is@p") .!lsp:is@p { list-style-position: inside  !important; }

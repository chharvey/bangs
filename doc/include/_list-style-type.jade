      li(class="!lst:ur@p") .!lst:ur@p { list-style-type: upper-roman          !important; }
      li(class="!lst:ua@p") .!lst:ua@p { list-style-type: upper-alpha          !important; }
      li(class="!lst:dc@p") .!lst:dc@p { list-style-type: decimal              !important; }
      li(class="!lst:la@p") .!lst:la@p { list-style-type: lower-alpha          !important; }
      li(class="!lst:lr@p") .!lst:lr@p { list-style-type: lower-roman          !important; }
      li(class="!lst:lg@p") .!lst:lg@p { list-style-type: lower-greek          !important; }
      li(class="!lst:d@p ") .!lst:d @p { list-style-type: disc                 !important; }
      li(class="!lst:c@p ") .!lst:c @p { list-style-type: circle               !important; }
